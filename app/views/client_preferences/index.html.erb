<p id="notice"><%= notice %></p>

<h1>Client Preferences</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @client_preferences.each do |client_preference| %>
      <tr>
        <td><%= link_to 'Show', client_preference %></td>
        <td><%= link_to 'Edit', edit_client_preference_path(client_preference) %></td>
        <td><%= link_to 'Destroy', client_preference, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Client Preference', new_client_preference_path %>
